program Singleton;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils,
  uFramework in 'uFramework.pas';

var
  con1, con2: TConexao;

begin
  con1:= TConexao.Create();
  con2:= TConexao.Create();
  if con1 = con2 then
    WriteLn('São a mesma instância');
  WriteLn('con1 = ' + con1.GetHashCode().ToString());
  WriteLn('con2 = ' + con2.GetHashCode().ToString());
  ReadLn;

end.
